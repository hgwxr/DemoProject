<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="tiangou"
            type="hgwxr.wl.com.basemvp.data.other.CaipuBase.TngouBean"/>
    </data>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="hgwxr.wl.com.basemvp.MainActivity">

    <TextView
        android:layout_centerInParent="true"
        android:background="@color/colorAccent"
        android:id="@+id/basemvp_tv_jump"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:text='@{tiangou.id+""}' />
    <TextView
        android:layout_centerInParent="true"
        android:background="@color/colorAccent"
       android:layout_below="@id/basemvp_tv_jump"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:text="@{tiangou.name}"  />
</RelativeLayout>
</layout>